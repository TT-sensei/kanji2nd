            {char:'ç‰›', reading:'ã†ã—'},{char:'é­š', reading:'ã•ã‹ãª'},{char:'äº¬', reading:'ãã‚‡ã†'},{char:'å¼·', reading:'ã¤ã‚ˆã„'},
            {char:'æ•™', reading:'ãŠã—ãˆã‚‹'},{char:'è¿‘', reading:'ã¡ã‹ã„'},{char:'å…„', reading:'ã‚ã«'},{char:'å½¢', reading:'ã‹ãŸã¡'},
            {char:'è¨ˆ', reading:'ã‘ã„'},{char:'å…ƒ', reading:'ã’ã‚“'},{char:'è¨€', reading:'ã„ã†'},{char:'åŸ', reading:'ã’ã‚“'},
            {char:'æˆ¸', reading:'ã¨'},{char:'å¤', reading:'ãµã‚‹ã„'},{char:'åˆ', reading:'ã”'},{char:'å¾Œ', reading:'ã‚ã¨'},
            {char:'èª', reading:'ã”'},{char:'å·¥', reading:'ã“ã†'},{char:'å…¬', reading:'ã“ã†'},{char:'åºƒ', reading:'ã²ã‚ã„'},
            {char:'äº¤', reading:'ã“ã†'},{char:'å…‰', reading:'ã²ã‹ã‚Š'},{char:'è€ƒ', reading:'ã‹ã‚“ãŒãˆã‚‹'},{char:'è¡Œ', reading:'ã„ã'},
            {char:'é«˜', reading:'ãŸã‹ã„'},{char:'åˆ', reading:'ã‚ã†'},{char:'å›½', reading:'ãã«'},{char:'é»’', reading:'ãã‚'},
            {char:'ä»Š', reading:'ã„ã¾'},{char:'æ‰', reading:'ã•ã„'},{char:'ç´°', reading:'ã»ãã„'},{char:'ä½œ', reading:'ã¤ãã‚‹'},
            {char:'ç®—', reading:'ã•ã‚“'},{char:'æ­¢', reading:'ã¨ã¾ã‚‹'},{char:'å¸‚', reading:'ã—'},{char:'çŸ¢', reading:'ã‚„'},
            {char:'å§‰', reading:'ã‚ã­'},{char:'æ€', reading:'ãŠã‚‚ã†'},{char:'ç´™', reading:'ã‹ã¿'},{char:'å¯º', reading:'ã¦ã‚‰'},
            {char:'è‡ª', reading:'ã˜'},{char:'æ™‚', reading:'ã˜'},{char:'å®¤', reading:'ã—ã¤'},{char:'ç¤¾', reading:'ã—ã‚ƒ'},
            {char:'å¼±', reading:'ã‚ˆã‚ã„'},{char:'é¦–', reading:'ãã³'},{char:'ç§‹', reading:'ã‚ã'},{char:'é€±', reading:'ã—ã‚…ã†'},
            {char:'æ˜¥', reading:'ã¯ã‚‹'},{char:'æ›¸', reading:'ã‹ã'},{char:'å°‘', reading:'ã™ã“ã—'},{char:'å ´', reading:'ã°'},
            {char:'è‰²', reading:'ã„ã‚'},{char:'é£Ÿ', reading:'ãŸã¹ã‚‹'},{char:'å¿ƒ', reading:'ã“ã“ã‚'},{char:'æ–°', reading:'ã‚ãŸã‚‰ã—ã„'},
            {char:'è¦ª', reading:'ãŠã‚„'},{char:'å›³', reading:'ãš'},{char:'æ•°', reading:'ã‹ãš'},{char:'è¥¿', reading:'ã«ã—'},
            {char:'å£°', reading:'ã“ãˆ'},{char:'æ˜Ÿ', reading:'ã»ã—'},{char:'æ™´', reading:'ã¯ã‚Œã‚‹'},{char:'åˆ‡', reading:'ãã‚‹'},
            {char:'é›ª', reading:'ã‚†ã'},{char:'èˆ¹', reading:'ãµã­'},{char:'ç·š', reading:'ã›ã‚“'},{char:'å‰', reading:'ã¾ãˆ'},
            {char:'çµ„', reading:'ãã¿'},{char:'èµ°', reading:'ã¯ã—ã‚‹'},{char:'å¤š', reading:'ãŠãŠã„'},{char:'å¤ª', reading:'ãµã¨ã„'},
            {char:'ä½“', reading:'ã‹ã‚‰ã '},{char:'å°', reading:'ã ã„'},{char:'åœ°', reading:'ã¡'},{char:'æ± ', reading:'ã„ã‘'},
            {char:'çŸ¥', reading:'ã—ã‚‹'},{char:'èŒ¶', reading:'ã¡ã‚ƒ'},{char:'æ˜¼', reading:'ã²ã‚‹'},{char:'é•·', reading:'ãªãŒã„'},
            {char:'é³¥', reading:'ã¨ã‚Š'},{char:'æœ', reading:'ã‚ã•'},{char:'ç›´', reading:'ãªãŠã™'},{char:'é€š', reading:'ã¨ãŠã‚‹'},
            {char:'å¼Ÿ', reading:'ãŠã¨ã†ã¨'},{char:'åº—', reading:'ã¿ã›'},{char:'ç‚¹', reading:'ã¦ã‚“'},{char:'é›»', reading:'ã§ã‚“'},
            {char:'åˆ€', reading:'ã‹ãŸãª'},{char:'å†¬', reading:'ãµã‚†'},{char:'å½“', reading:'ã¨ã†'},{char:'æ±', reading:'ã²ãŒã—'},
            {char:'ç­”', reading:'ã“ãŸãˆ'},{char:'é ­', reading:'ã‚ãŸã¾'},{char:'åŒ', reading:'ãŠãªã˜'},{char:'é“', reading:'ã¿ã¡'},
            {char:'èª­', reading:'ã‚ˆã‚€'},{char:'å†…', reading:'ã†ã¡'},{char:'å—', reading:'ã¿ãªã¿'},{char:'è‚‰', reading:'ã«ã'},
            {char:'é¦¬', reading:'ã†ã¾'},{char:'å£²', reading:'ã†ã‚‹'},{char:'è²·', reading:'ã‹ã†'},{char:'éº¦', reading:'ã‚€ã'},
            {char:'åŠ', reading:'ã¯ã‚“'},{char:'ç•ª', reading:'ã°ã‚“'},{char:'çˆ¶', reading:'ã¡ã¡'},{char:'é¢¨', reading:'ã‹ãœ'},
            {char:'åˆ†', reading:'ã‚ã‘ã‚‹'},{char:'è', reading:'ãã'},{char:'ç±³', reading:'ã“ã‚'},{char:'æ­©', reading:'ã‚ã‚‹ã'},
            {char:'æ¯', reading:'ã¯ã¯'},{char:'æ–¹', reading:'ã»ã†'},{char:'åŒ—', reading:'ããŸ'},{char:'æ¯', reading:'ã¾ã„'},
            {char:'å¦¹', reading:'ã„ã‚‚ã†ã¨'},{char:'ä¸‡', reading:'ã¾ã‚“'},{char:'æ˜', reading:'ã‚ã‹ã‚‹ã„'},{char:'é³´', reading:'ãªã'},
            {char:'æ¯›', reading:'ã‘'},{char:'é–€', reading:'ã‚‚ã‚“'},{char:'å¤œ', reading:'ã‚ˆã‚‹'},{char:'é‡', reading:'ã®'},
            {char:'å‹', reading:'ã¨ã‚‚'},{char:'ç”¨', reading:'ã‚ˆã†'},{char:'æ›œ', reading:'ã‚ˆã†'},{char:'æ¥', reading:'ãã‚‹'},
            {char:'é‡Œ', reading:'ã•ã¨'},{char:'ç†', reading:'ã‚Š'},{char:'è©±', reading:'ã¯ãªã—'}
        ];

        // çŠ¶æ…‹ç®¡ç† (ä¿å­˜ã‚­ãƒ¼ã‚’2å¹´ç”Ÿç”¨ã«å¤‰æ›´)
        let progressPractice = JSON.parse(localStorage.getItem('kanjiMasterPractice_Grade2')) || {};
        let progressTest = JSON.parse(localStorage.getItem('kanjiMasterTest_Grade2')) || {};
        
        let writer;
        let currentChar = null;
        let currentMode = 'practice'; // 'practice' or 'test'

        // --- ç”»é¢é·ç§» ---
        function showScreen(screenId) {
            document.querySelectorAll('.screen').forEach(s => s.classList.remove('active'));
            document.getElementById(screenId).classList.add('active');
            
            if (screenId === 'list-screen') {
                renderList();
            }
        }

        // --- ãƒ¢ãƒ¼ãƒ‰é¸æŠ ---
        function selectMode(mode) {
            currentMode = mode;
            showScreen('list-screen');
        }

        // --- ä¸€è¦§ç”»é¢ã®æç”» ---
        function renderList() {
            const grid = document.getElementById('kanji-grid');
            grid.innerHTML = '';
            
            // ãƒ¢ãƒ¼ãƒ‰ã”ã¨ã®è¡¨ç¤ºåˆ‡ã‚Šæ›¿ãˆ
            const badge = document.getElementById('mode-display');
            const counter = document.getElementById('star-counter');
            let count = 0;
            const targetProgress = (currentMode === 'practice') ? progressPractice : progressTest;

            if (currentMode === 'practice') {
                badge.textContent = "â­ ã‚Œã‚“ã—ã‚…ã†ãƒ¢ãƒ¼ãƒ‰";
                badge.style.backgroundColor = "#FFA726"; // ç·´ç¿’è‰²
            } else {
                badge.textContent = "ğŸ‘‘ ãƒ†ã‚¹ãƒˆãƒ¢ãƒ¼ãƒ‰";
                badge.style.backgroundColor = "#42A5F5"; // ãƒ†ã‚¹ãƒˆè‰²
            }

            kanjiData.forEach(item => {
                const isCleared = targetProgress[item.char];
                if (isCleared) count++;

                const card = document.createElement('div');
                card.className = `kanji-card`;
                if (isCleared) {
                    card.classList.add(currentMode === 'practice' ? 'cleared-practice' : 'cleared-test');
                }
                
                card.innerHTML = `
                    ${item.char}
                    <div class="mark star-mark">â­</div>
                    <div class="mark crown-mark">ğŸ‘‘</div>
                `;
                
                card.onclick = () => startApp(item);
                grid.appendChild(card);
            });

            counter.innerText = count;
        }

        // --- ã‚¢ãƒ—ãƒªé–‹å§‹ï¼ˆç·´ç¿’orãƒ†ã‚¹ãƒˆï¼‰ ---
        function startApp(item) {
            currentChar = item;
            showScreen('practice-screen');

            // èƒŒæ™¯è‰²ã¨ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸ã®åˆ‡ã‚Šæ›¿ãˆ
            const screen = document.getElementById('practice-screen');
            const msgArea = document.getElementById('message-area');
            const readingDisplay = document.getElementById('current-reading');
            
            if (currentMode === 'test') {
                screen.classList.add('test-bg');
                msgArea.innerText = "ã“ã® ã‹ã‚“ã˜ ã‚’ ã‹ã“ã†ï¼";
                msgArea.style.color = "#0277BD"; // æ¿ƒã„é’
                
                readingDisplay.innerText = item.reading; 
                readingDisplay.style.fontSize = "3rem";
            } else {
                screen.classList.remove('test-bg');
                msgArea.innerText = "ã†ã™ã„ã›ã‚“ã‚’ ãªãã‚ã†ï¼";
                msgArea.style.color = "#EF6C00"; // æ¿ƒã„ã‚ªãƒ¬ãƒ³ã‚¸
                
                readingDisplay.innerText = `${item.char} (${item.reading})`;
                readingDisplay.style.fontSize = "2rem";
            }

            // UIãƒªã‚»ãƒƒãƒˆ
            document.getElementById('character-target-div').innerHTML = '';
            document.getElementById('anim-star').style.display = 'none';
            document.getElementById('anim-crown').style.display = 'none';

            // HanziWriter è¨­å®š
            const isTest = (currentMode === 'test');

            writer = HanziWriter.create('character-target-div', item.char, {
                width: 300,
                height: 300,
                padding: 10,
                showOutline: !isTest, // ãƒ†ã‚¹ãƒˆãªã‚‰ä¸‹çµµãªã—
                strokeAnimationSpeed: 1,
                delayBetweenStrokes: 200,
                radicalColor: '#2E7D32', // å°‘ã—å¤§äººã£ã½ã„ç·‘
                drawingWidth: 25,
                outlineColor: '#DDD',
                strokeColor: '#333',
                highlightColor: '#FF7043',
                showHintAfterMisses: 1 
            });

            startQuiz();
        }

        // --- ã‚¯ã‚¤ã‚ºãƒ­ã‚¸ãƒƒã‚¯ ---
        function startQuiz() {
            writer.quiz({
                onMistake: function(strokeData) {
                    const msg = document.getElementById('message-area');
                    if (currentMode === 'test') {
                        msg.innerText = "ãŠã—ã„ï¼ ãƒ’ãƒ³ãƒˆã‚’ã¿ã¦ã­";
                    } else {
                        msg.innerText = "ã“ã“ã ã‚ˆï¼";
                    }
                    msg.style.color = "#D32F2F";
                },
                onCorrectStroke: function(strokeData) {
                    const msg = document.getElementById('message-area');
                    msg.innerText = "ã„ã„ã­ï¼";
                    msg.style.color = "#388E3C";
                },
                onComplete: function(summaryData) {
                    handleComplete();
                }
            });
        }

        // --- ã‚¯ãƒªã‚¢å‡¦ç† ---
        function handleComplete() {
            const msg = document.getElementById('message-area');
            
            if (currentMode === 'practice') {
                msg.innerText = "ã§ããŸãƒ¼ï¼ â­ã‚²ãƒƒãƒˆï¼";
                document.getElementById('anim-star').style.display = 'block';
                progressPractice[currentChar.char] = true;
                localStorage.setItem('kanjiMasterPractice_Grade2', JSON.stringify(progressPractice));
            } else {
